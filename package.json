  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "lodash.set": "^4.3.2",
    "reconnecting-websocket": "^4.4.0",
    "uptill": "^1.0.1",
    "ws": "^7.4.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "browserify": "^17.0.0",
    "cheerio": "^1.0.0-rc.5",
    "cssnano": "^4.1.10",
    "haikunator": "^2.1.2",
    "listdirs": "^3.1.1",
    "lodash.topath": "^4.5.2",
    "mustache": "^4.1.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.3",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "vue": "^2.6.12"
  }
